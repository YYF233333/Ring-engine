; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

_global_script_classes=[{
"base": "RefCounted",
"class": &"Assert",
"language": &"GDScript",
"path": "res://modules/utils/assert.gd"
}, {
"base": "Layers",
"class": &"ImageGroup",
"language": &"GDScript",
"path": "res://modules/character/image_group.gd"
}, {
"base": "Sprite2D",
"class": &"Layer",
"language": &"GDScript",
"path": "res://modules/canvas_layer/layer.gd"
}, {
"base": "Node2D",
"class": &"Layers",
"language": &"GDScript",
"path": "res://modules/canvas_layer/layers.gd"
}, {
"base": "ProductType",
"class": &"Option",
"language": &"GDScript",
"path": "res://modules/utils/option.gd"
}, {
"base": "RefCounted",
"class": &"ProductType",
"language": &"GDScript",
"path": "res://modules/utils/product_type.gd"
}, {
"base": "ProductType",
"class": &"Result",
"language": &"GDScript",
"path": "res://modules/utils/result.gd"
}, {
"base": "RefCounted",
"class": &"Transform",
"language": &"GDScript",
"path": "res://modules/animation/transform/transform.gd"
}]
_global_script_class_icons={
"Assert": "",
"ImageGroup": "",
"Layer": "",
"Layers": "",
"Option": "",
"ProductType": "",
"Result": "",
"Transform": ""
}

[application]

config/name="Ring Engine"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.0", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

SnapshotServer="*res://modules/persist/snapshot.gd"
GlobalVar="*res://modules/persist/global_var.gd"
TextboxServer="*res://modules/textbox/textbox_server.gd"
AssetLoader="*res://modules/loading/asset_loader.gd"
Config="*res://modules/config/config.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="Ring Engine"

[editor]

export/convert_text_resources_to_binary=true

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/godotsize/plugin.cfg", "res://addons/script-name-on-top/plugin.cfg")

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

[input]

gdshell_toggle_ui={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":96,"physical_keycode":0,"unicode":0,"echo":false,"script":null)
]
}

[rendering]

renderer/rendering_method="mobile"
